{
    "env": {
        "node": true,
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": "latest"
  },
  "plugins": [
      "@typescript-eslint",
      "react"
  ],
    "rules": {
      "no-underscore-dangle": ["error",{"allow": ["_id"]}],
      "no-console": "warn"
    }
  }
